          <activationType>P</activationType>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </start>
      </routeNodes>
    </documentType>

   <documentType>
      <name>KualiOrganizationContextMaintenanceDocument</name>
      <description>Create or Update a Organization Context</description>
      <label>Kuali Organization Context Maintenance Document</label>
      <postProcessorName>org.kuali.rice.kns.workflow.postprocessor.KualiPostProcessor</postProcessorName>
