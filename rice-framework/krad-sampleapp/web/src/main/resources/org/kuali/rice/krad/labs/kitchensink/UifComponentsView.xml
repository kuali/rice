                <bean parent="UifCompView-KeyValueLocation" p:key="UifCompView-Page2" p:value="Other Fields"
                      p:location.pageId="UifCompView-Page2"/>
                <bean parent="UifCompView-KeyValueLocation" p:key="UifCompView-Page3" p:value="Validation"
