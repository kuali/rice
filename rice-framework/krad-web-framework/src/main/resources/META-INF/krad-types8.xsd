          <xsd:documentation source="org.kuali.rice.krad.datadictionary.validation.constraint.CaseConstraint" xml:lang="en"><![CDATA[gets the property name for the attribute to which the case constraint is applied to]]></xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="messageNamespaceCode" type="xsd:string">
        <xsd:annotation>
