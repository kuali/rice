    <xsd:attribute name="componentCode">
      <xsd:annotation>
        <xsd:documentation source="org.kuali.rice.krad.uif.layout.StackedLayoutManagerBase" xml:lang="en"><![CDATA[documentation not available]]></xsd:documentation>
      </xsd:annotation>
