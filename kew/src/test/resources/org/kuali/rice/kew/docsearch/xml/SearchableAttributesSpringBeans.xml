<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
  
  <!--  RouteQueueService -->
  <bean id="enRouteQueueService" class="org.kuali.rice.kew.routequeue.RouteQueueServiceImpl">
		<property name="routeQueueDAO">
			<ref bean="enRouteQueueDAO" />
		</property>
	</bean>
	<bean id="enRouteQueueDAO" class="org.kuali.rice.kew.routequeue.dao.RouteQueueDAOOjbImpl">
		<property name="dataSource">
			<ref bean="enWorkflowDataSource" />
		</property>
  </bean>

</beans>