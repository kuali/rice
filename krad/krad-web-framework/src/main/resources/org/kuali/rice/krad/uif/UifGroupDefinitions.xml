              value="@{#ConfigProperties['krad.externalizable.images.url']}h2_expand.png"/>
    <property name="instructionalMessageField">
      <bean parent="Uif-InstructionalMessage"/>
    </property>
    <property name="readOnly" value="@{#parent.readOnly}"/>
  </bean>

