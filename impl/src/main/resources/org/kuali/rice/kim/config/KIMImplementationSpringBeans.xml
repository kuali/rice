                           http://www.springframework.org/schema/aop/spring-aop-2.0.xsd">

    <bean id="kimDataSource" class="org.kuali.rice.core.database.PrimaryDataSourceFactoryBean" lazy-init="true">
        <property name="preferredDataSourceParams">
            <list>
                <value>kim.datasource</value>
            </list>
        </property>
        <property name="preferredDataSourceJndiParams">
            <list>
                <value>kim.datasource.jndi.location</value>
            </list>
        </property>
        <property name="serverDataSource" value="true"/>
    </bean>

	<bean id="kimOjbConfigurer" class="org.kuali.rice.core.ojb.BaseOjbConfigurer">
		<property name="jcdAliases">
			<list>
				<value>kimDataSource</value>
			</list>
		</property>
		<property name="metadataLocation" value="classpath:org/kuali/rice/kim/config/OJB-repository-kim.xml" />
	</bean>


    <bean id="kimPlatformAwareDao" abstract="true" class="org.kuali.rice.kns.dao.impl.PlatformAwareDaoBaseOjb">
        <property name="jcdAlias" value="kimDataSource" />
        <property name="dbPlatform" ref="dbPlatform" />
    </bean>

	<bean id="kimRoleDao" class="org.kuali.rice.kim.dao.impl.KimRoleDaoOjb" parent="kimPlatformAwareDao" />
	<bean id="kimGroupDao" class="org.kuali.rice.kim.dao.impl.KimGroupDaoOjb" parent="kimPlatformAwareDao" />
	<bean id="kimPermissionDao" class="org.kuali.rice.kim.dao.impl.KimPermissionDaoOjb" parent="kimPlatformAwareDao" />


    <bean id="kimSoapService" class="org.kuali.rice.ksb.messaging.SOAPServiceDefinition" abstract="true">
       <property name="serviceNameSpaceURI" value="KIM"/>
       <property name="jaxWsService" value="false"/>
       <property name="priority" value="3" />
       <property name="retryAttempts" value="0" />
       <property name="busSecurity" value="true"/>
    </bean>

    <bean id="kimJaxWsSoapService" class="org.kuali.rice.ksb.messaging.SOAPServiceDefinition" abstract="true">
       <property name="serviceNameSpaceURI" value="http://rice.kuali.org/wsdl/kim"/>
       <property name="jaxWsService" value="true"/>
       <property name="priority" value="3" />
       <property name="retryAttempts" value="0" />
       <property name="busSecurity" value="${kim.soapExposedService.jaxws.security}"/>
    </bean>

    <bean id="kimService" class="org.kuali.rice.ksb.messaging.JavaServiceDefinition" abstract="true">
      <property name="serviceNameSpaceURI" value="KIM" />
      <property name="retryAttempts" value="0" />
    </bean>

  <!-- START JPA CONFIGURATION -->

  <bean id="kimEntityManagerFactory" class="org.kuali.rice.core.jpa.spring.RiceEntityManagerProxyFactoryBean">
    <constructor-arg value="rice.kim"/>
    <constructor-arg ref="kimDataSource"/>
  </bean>

  <bean id="kimEntityManager" class="org.springframework.orm.jpa.support.SharedEntityManagerBean">
  	<property name="entityManagerFactory" ref="kimEntityManagerFactory"/>
  </bean>

  <!-- END JPA CONFIGURATION -->

  <bean id="kimAuthenticationService" class="org.kuali.rice.kim.service.impl.AuthenticationServiceImpl"/>

  <!-- 
        Authentication Service
  -->
  <bean class="org.kuali.rice.ksb.messaging.KSBExporter">
	<property name="remotedServiceRegistry" ref="enServiceInvoker"/>
    <property name="serviceDefinition">
      <bean parent="kimService"
        p:service-ref="kimAuthenticationService"
        p:localServiceName="kimAuthenticationService" />
    </property>
  </bean>
  <bean class="org.kuali.rice.ksb.messaging.PropertyConditionalKSBExporter">
	<property name="remotedServiceRegistry" ref="enServiceInvoker"/>
    <property name="serviceDefinition">
      <bean parent="kimSoapService"
        p:service-ref="kimAuthenticationService"
        p:localServiceName="kimAuthenticationServiceSOAP" />
    </property>
    <property name="propertyName" value="kimAuthenticationServiceSOAP.expose"/>
  </bean>

