                <bean parent="UifCompView-KeyValueLocation" p:key="UifCompView-Page4" p:value="Validation - Regex"
                      p:location.pageId="UifCompView-Page4"/>
                <bean parent="UifCompView-KeyValueLocation" p:key="UifCompView-Page5" p:value="Progressive Disclosure"
                      p:location.pageId="UifCompView-Page5"/>
                <bean parent="UifCompView-KeyValueLocation" p:key="UifCompView-Page6" p:value="Lookups, etc"
                      p:location.pageId="UifCompView-Page6"/>
                <bean parent="UifCompView-KeyValueLocation" p:key="UifCompView-Page7" p:value="Collections"
                      p:location.pageId="UifCompView-Page7"/>
                <bean parent="UifCompView-KeyValueLocation" p:key="UifCompView-Page8" p:value="Button/Header Levels"
                      p:location.pageId="UifCompView-Page8"/>
                <bean parent="UifCompView-KeyValueLocation" p:key="UifCompView-Page9" p:value="Authorization"
                      p:location.pageId="UifCompView-Page9"/>
                <bean parent="UifCompView-KeyValueLocation" p:key="UifCompView-Page10" p:value="Other Examples"
                      p:location.pageId="UifCompView-Page10"/>
                <bean parent="UifCompView-KeyValueLocation" p:key="UifCompView-Page11" p:value="Event Examples"
                      p:location.pageId="UifCompView-Page11"/>
              </list>
            </property>
          </bean>
        </property>
      </bean>
    </property>
    <property name="footer">
      <bean parent="Uif-FormFooter"/>
    </property>
    <property name="stickyFooter" value="true"/>
  </bean>

  <bean id="UifCompView-KeyValueLocation" parent="Uif-KeyValueLocation">
