  <!--
  ********************************************************************************************************************
                                           UIF Components View (Kitchen Sink)
  ********************************************************************************************************************
  -->
  <bean id="UifCompView" parent="Uif-FormView" p:additionalCssClasses="testClass1,testClass2">
    <property name="parentLocation">
      <bean parent="Uif-RicePortalParentLocation"/>
    </property>
    <property name="breadcrumbs.usePathBasedBreadcrumbs" value="true"/>
    <property name="stickyHeader" value="true"/>
    <property name="stickyBreadcrumbs" value="true"/>
    <property name="topGroup">
      <bean parent="Uif-VerticalBoxGroup" p:style="background-color: lightBlue; width: 100%;">
        <property name="items">
          <list>
            <bean parent="Uif-Message" p:messageText="Top Group Content" p:style="font-size: 24px;"
                    p:additionalCssClasses="addTestClass">
              <property name="cssClasses">
                <list merge="true">
                  <value>mergeTestClass</value>
                </list>
              </property>
            </bean>
          </list>
        </property>
      </bean>
    </property>
    <property name="header.areaTitleText" value="[color='red']KRAD[/color] Kitchen Sink (areaText)"/>
    <property name="header.metadataText" value="Metadata Text"/>
    <property name="header.rightGroup">
      <bean parent="Uif-HorizontalBoxGroup" p:style="background-color: lightBlue; padding: 5px;">
        <property name="items">
          <list>
            <bean parent="Uif-Message" p:messageText="RightGroup Content" />
          </list>
        </property>
      </bean>
    </property>
    <property name="header.lowerGroup">
      <bean parent="Uif-VerticalBoxGroup" p:style="background-color: lightGray; padding: 5px;">
        <property name="items">
          <list>
            <bean parent="Uif-Message" p:messageText="lowerGroup content"/>
          </list>
        </property>
      </bean>
    </property>
    <property name="header.upperGroup">
      <bean parent="Uif-HorizontalBoxGroup" p:style="background-color: lightGray; padding: 5px;">
        <property name="items">
          <list>
            <bean parent="Uif-Message" p:messageText="upperGroup content"/>
          </list>
        </property>
      </bean>
    </property>
    <property name="help">
      <bean parent="Uif-Help" p:externalHelpUrl="http://www.kuali.org/"/>
    </property>
    <property name="entryPageId" value="UifCompView-Page1"/>
    <property name="navigation">
      <ref bean="UifCompView-Menu"/>
    </property>
    <property name="items">
      <list>
        <bean id="UifCompView-Page1" parent="UifCompViewPageWithFooter">
          <property name="help">
            <bean parent="Uif-Help">
              <property name="helpDefinition">
                <bean parent="HelpDefinition" p:parameterNamespace="KR-SAP" p:parameterName="TEST_PARAM"
                      p:parameterDetailType="TEST_COMPONENT"/>
              </property>
            </bean>
          </property>
          <property name="items">
            <list>
              <ref bean="UifCompView-AnchorLinks"/>
              <ref bean="UifCompView-TextFields"/>
              <ref bean="UifCompView-DateFields"/>
              <ref bean="UifCompView-SelectFields"/>
              <ref bean="UifCompView-OtherControls"/>
              <bean parent="Uif-JumpToTopLink" id="TopLink1"/>
            </list>
          </property>
        </bean>

        <bean id="UifCompView-Page2" parent="UifCompViewPageWithFooter">
          <property name="items">
            <list>
              <ref bean="UifCompView-FieldGroups"/>
