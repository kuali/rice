          <xsd:documentation source="org.kuali.rice.krad.uif.util.ColumnCalculationInfo" xml:lang="en"><![CDATA[Gets the column number.  This should not be set through configuration as it is overridden by the
 propertyName's caclculated column number.  Do not set through xml configuration]]></xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="pageTotalField" type="org.kuali.rice.krad.uif.field.MessageField">
        <xsd:annotation>
          <xsd:documentation source="org.kuali.rice.krad.uif.util.ColumnCalculationInfo" xml:lang="en"><![CDATA[Gets the pageTotalField.  This field is the field which holds the pageTotal for the column
 and specifies its label. This SHOULD NOT BE SET except by the base bean (in MOST cases).]]></xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="calculationFunctionExtraData" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="org.kuali.rice.krad.uif.util.ColumnCalculationInfo" xml:lang="en"><![CDATA[This specifies extra data to be sent to the calculation function.  This can be any valid javascript value
 (number, string, JSON - for passing multiple settings, etc).
 
 The function specified by calculationFunctionName MUST take a second parameter when using this option.]]></xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="calculationOnKeyUp" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="org.kuali.rice.krad.uif.util.ColumnCalculationInfo" xml:lang="en"><![CDATA[documentation not available]]></xsd:documentation>
        </xsd:annotation>
