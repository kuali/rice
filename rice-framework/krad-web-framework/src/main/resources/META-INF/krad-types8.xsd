    <xsd:attribute name="validationMessageParams">
      <xsd:annotation>
        <xsd:documentation source="org.kuali.rice.krad.datadictionary.validation.constraint.CaseConstraint" xml:lang="en"><![CDATA[Parameters to be used in the string retrieved by this constraint's messageKey, ordered by number of
 the param]]></xsd:documentation>
